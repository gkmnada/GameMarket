{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "gameRead": {
        "ClusterId": "game",
        "Match": {
          "Path": "/game/{**catch-all}",
          "Methods": [ "GET" ]
        },
        "Transforms": [
          {
            "PathPattern": "api/game/{**catch-all}"
          }
        ]
      },
      "gameWrite": {
        "ClusterId": "game",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/game/{**catch-all}",
          "Methods": [ "POST", "PUT", "DELETE" ]
        },
        "Transforms": [
          {
            "PathPattern": "api/game/{**catch-all}"
          }
        ]
      },
      "search": {
        "ClusterId": "search",
        "Match": {
          "Path": "/search/{**catch-all}",
          "Methods": [ "GET" ]
        },
        "Transforms": [
          {
            "PathPattern": "api/search/{**catch-all}"
          }
        ]
      },
      "filter": {
        "ClusterId": "filter",
        "Match": {
          "Path": "/filter/{**catch-all}",
          "Methods": [ "POST" ]
        },
        "Transforms": [
          {
            "PathPattern": "api/filter/{**catch-all}"
          }
        ]
      }
    }
  }
}
